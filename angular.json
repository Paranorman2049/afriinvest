{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "afriinvest": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/afriinvest",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/images"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/vendor/bootstrap-icons/font/bootstrap-icons.css",
              "src/assets/vendor/hs-mega-menu/dist/hs-mega-menu.min.css",
              "src/assets/vendor/aos/dist/aos.css",
              "src/assets/vendor/swiper/swiper-bundle.min.css",
              "src/assets/css/snippets.min.css",
              "src/assets/vendor/leaflet/dist/leaflet.css",
              "src/assets/css/theme.min.css"
            ],
            "scripts": [
              "src/assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/vendor/hs-header/dist/hs-header.min.js",
              "src/assets/vendor/hs-mega-menu/dist/hs-mega-menu.min.js",
              "src/assets/vendor/hs-show-animation/dist/hs-show-animation.min.js",
              "src/assets/vendor/hs-go-to/dist/hs-go-to.min.js",
              "src/assets/vendor/hs-nav-scroller/dist/hs-nav-scroller.min.js",
              "src/assets/vendor/hs-sticky-block/dist/hs-sticky-block.min.js",
              "src/assets/vendor/aos/dist/aos.js",
              "src/assets/vendor/typed.js/lib/typed.min.js",
              "src/assets/vendor/swiper/swiper-bundle.min.js",
              "src/assets/vendor/leaflet/dist/leaflet.js",
              "src/assets/js/theme.min.js",
              "src/assets/vendor/smtpjs/smtpjs.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "afriinvest:build:production"
            },
            "development": {
              "browserTarget": "afriinvest:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "afriinvest:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "afriinvest"
}
